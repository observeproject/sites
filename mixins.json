{
    "mixins": [
        {
            "name": "kubernetes",
            "source": "https://github.com/kubernetes-monitoring/kubernetes-mixin",
            "subdir": "",
            "customfile": "kubernetes-custom.libsonnet",
            "description": "A set of Grafana dashboards and Prometheus alerts for Kubernetes."
        },
        {
            "name": "mimir",
            "source": "https://github.com/grafana/mimir",
            "subdir": "operations/mimir-mixin",
            "customfile": "mimir-custom.libsonnet",
